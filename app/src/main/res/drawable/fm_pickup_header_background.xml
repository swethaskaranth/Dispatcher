<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item >
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">

            <stroke android:width="1dp"
                android:color="@color/border_blue"/>

            <solid android:color="@color/solitude"/>


            <corners android:radius="@dimen/margin_5"/>

        </shape>
    </item>
    <item android:top="3dp" android:right="3dp" android:bottom="0dp"
        android:left="3dp">
        <shape
            android:shape="rectangle">
            <solid android:color="@color/solitude"/>
            <corners android:topLeftRadius="@dimen/margin_5"
                android:topRightRadius="@dimen/margin_5"/>

        </shape>
    </item>
</layer-list>
