<resources>
    <attr name="hintText" format="string" />
    <attr name="highlightSelected" format="boolean" />
    <attr name="highlightColor" format="color" />
    <attr name="textColor" format="color" />
    <attr name="clearText" format="string" />

    <declare-styleable name="MultiSpinnerSearch">
        <attr name="hintText" />
        <attr name="highlightSelected" />
        <attr name="highlightColor" />
        <attr name="textColor" />
        <attr name="clearText" />
    </declare-styleable>
    <declare-styleable name="SingleSpinnerSearch">
        <attr name="hintText" />
        <attr name="highlightSelected" />
        <attr name="highlightColor" />
        <attr name="textColor" />
        <attr name="clearText" />
    </declare-styleable>
    <declare-styleable name="RelativeRadioGroup">
        <attr name="showText" format="boolean" />
        <attr name="labelPosition" format="enum">
            <enum name="left" value="0"/>
            <enum name="right" value="1"/>
        </attr>
    </declare-styleable>
</resources>